      </Text>
      <Text fill="WHITE" layoutX="14.0" layoutY="220.0" strokeType="OUTSIDE" strokeWidth="0.0" text="New Password:">
         <font>
            <Font size="14.0" />
         </font>
      </Text>
      <Button layoutX="433.0" layoutY="252.0" mnemonicParsing="false" onAction="#handleSave" text="Save" />
      <Button layoutX="352.0" layoutY="252.0" mnemonicParsing="false" onAction="#handleCancel" text="Cancel" />
      <PasswordField fx:id="newPasswordField" layoutX="135.0" layoutY="196.0" promptText="Leave blank for same" />
