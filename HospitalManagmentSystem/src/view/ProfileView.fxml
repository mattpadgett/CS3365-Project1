<AnchorPane prefHeight="300.0" prefWidth="500.0" style="-fx-border-color: white;" stylesheets="@Style.css" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.ProfileViewController">
